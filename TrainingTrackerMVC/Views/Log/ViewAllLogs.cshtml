@model System.Collections.Generic.List<Models.Log>

@{
    ViewBag.Title = "View Logs";
}

<h2>View Logs</h2>

<div>
    <table>
        <tr>
            <th>
                Title
            </th>
            <th>
                Date Added
            </th>
        </tr>
        @foreach (var log in Model)
        {
            <tr>
                <td>
                    @log.Title
                </td>
                <td>
                    @log.DateAdded
                </td>
            </tr>
        }
    </table>
</div>
