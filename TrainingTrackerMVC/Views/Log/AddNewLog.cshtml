@model Models.Log

@{
    ViewBag.Title = "Index";
}

<h2>Logs</h2>

@using (Html.BeginForm("Register", "Account", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
    <div id="add-new-log">
        @Html.LabelFor(m => m.Title)
        @Html.TextBoxFor(m => m.Title)
    </div>

    <div>
        <input type="submit" value="Create New Log" />
    </div>
}